package com.vo;

import java.util.ArrayList;

public class SNSMessageSet {
	// 메시지
	private SNSMessageVO msgVO;
	// 해당 메시지에 대한 댓글 목록
	private ArrayList<SNSReplyVO> rlist = new ArrayList<>();
	public SNSMessageVO getMsgVO() {
		return msgVO;
	}
	public void setMsgVO(SNSMessageVO msgVO) {
		this.msgVO = msgVO;
	}
	public ArrayList<SNSReplyVO> getRlist() {
		return rlist;
	}
	public void setRlist(ArrayList<SNSReplyVO> rlist) {
		this.rlist = rlist;
	}
}
