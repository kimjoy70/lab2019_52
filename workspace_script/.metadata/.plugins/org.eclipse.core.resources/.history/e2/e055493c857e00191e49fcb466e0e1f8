<script>
	$(document).ready(function (){
		$('h1').addClass('high-light');
		$('h1').each(function (index, item){
			//$(item).addClass('high-lignt-' + index);
			$(this).addClass('high-lignt-' + index);			
		});
	});
</script>
<h1>item - 0</h1>
